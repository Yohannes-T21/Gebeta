{
  "name": "ethiopian-recipe-share-backend",
  "version": "1.0.0",
  "description": "Backend for Ethiopian Recipe Share application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:search": "ts-node --require tsconfig-paths/register scripts/testSearch.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "ts-node --require tsconfig-paths/register src/scripts/migrate.ts",
    "seed": "ts-node --require tsconfig-paths/register src/scripts/seed.ts",
    "seed:users": "ts-node --require tsconfig-paths/register scripts/seedUsers.ts",
    "seed:recipes": "ts-node --require tsconfig-paths/register scripts/seedRecipes.ts",
    "seed:restaurants": "ts-node --require tsconfig-paths/register scripts/seedRestaurants.ts",
    "seed:all": "npm run seed:users && npm run seed:restaurants && npm run seed:recipes"
  },
  "dependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.15.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "_moduleAliases": {
    "@": "./src",
    "@config": "./src/config",
    "@controllers": "./src/controllers",
    "@middlewares": "./src/middlewares",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@services": "./src/services",
    "@types": "./src/types",
    "@utils": "./src/utils",
    "@validations": "./src/validations"
  }
}
